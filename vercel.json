{
  "version": 2,
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/panel",
      "destination": "/panel.html"
    },
    {
      "source": "/projekty",
      "destination": "/projekty.html"
    },
    {
      "source": "/client-dashboard/:path*",
      "destination": "https://client-dashboard-ecm.vercel.app/:path*"
    },
    {
      "source": "/panel-klienta/:path*",
      "destination": "https://client-dashboard-ecm.vercel.app/:path*"
    }
  ]
}