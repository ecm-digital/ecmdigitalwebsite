{
  "version": 2,
  "buildCommand": "echo 'Building static site'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "rewrites": [
    {
      "source": "/dashboard/:path*",
      "destination": "https://client-dashboard-9khfoip7j-ecm-digitals-projects.vercel.app/:path*"
    },
    {
      "source": "/dashboard",
      "destination": "https://client-dashboard-9khfoip7j-ecm-digitals-projects.vercel.app/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}